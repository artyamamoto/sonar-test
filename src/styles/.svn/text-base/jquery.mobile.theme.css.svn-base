/**
 * jquery mobile テーマ
 */

/**
 * Theme : h (header)
 */
.ui-bar-h {
	border: 1px solid 		#ffffff;
	background: 			#000000;
	color: 					#fff;
	font-weight:            normal;
	text-shadow:            none;
}
.ui-bar-h,
.ui-bar-h input,
.ui-bar-h select,
.ui-bar-h textarea,
.ui-bar-h button {
    font-family: 'Artbank Gothic', Helvetica, Arial, sans-serif;
}
.ui-bar-h .ui-link-inherit {
	color: 					#900;
}
.ui-bar-h .ui-link {
	color: 					#42555b;
	font-weight:            normal;
}
.ui-bar-h h1.ui-title {
    text-align: left;
    margin-left: 1em;
}

.ui-body-h {
	border: 1px solid 		#C6C6C6;
	background: 			#ffffff;
	color: 					#333333;
	text-shadow:            none;
	font-weight:            normal;
}
.ui-body-h,
.ui-body-h input,
.ui-body-h select,
.ui-body-h textarea,
.ui-body-h button {
	font-family: Helvetica, Arial, sans-serif;
}
.ui-body-h .ui-link-inherit {
	color: 					#333333;
}
.ui-body-h .ui-link {
	color: 					#2489CE;
	font-weight:            normal;
}
.ui-btn-up-h {
	border: 1px solid 		#42555b;
	background: 			#52656b;
	font-weight:            normal;
	color: 					#fff;
	text-shadow:            0 -1px 1px #aaa;
}
.ui-btn-up-h a.ui-link-inherit {
	color: 					#432f00;
}
.ui-btn-hover-h {
	border: 1px solid 		#666;
	background: 			#908365;
	font-weight:            normal;
	color: 					#fff;
	text-shadow: 0 -1px 1px #574313;
}
.ui-btn-hover-h a.ui-link-inherit {
	color: 					#fff;
}
.ui-btn-down-h {
	border: 1px solid 		#666;
	background: 			#908365;
	font-weight:            normal;
	color: 					#fff;
	text-shadow:            none;
}
.ui-btn-down-h a.ui-link-inherit {
	color: 					#fff;
}
.ui-btn-up-h,
.ui-btn-hover-h,
.ui-btn-down-h {
	font-family: Helvetica, Arial, sans-serif;
	text-decoration: none;
}

