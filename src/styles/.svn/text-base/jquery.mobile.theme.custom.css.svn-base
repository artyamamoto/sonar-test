/**
 * jquery mobile テーマ
 */

/*overlay*/
.top{
    background:transparent url(../images_sp/top.jpg) no-repeat scroll 0%;
    background-size:100%;
    width:70%;
    position:relative;
    left:15%;
}

/*play*/
.box_player {
 margin: 0 auto 50px auto;
 width: 740px;
 text-align: center;
}

/*font style*/

.brown {
    color: #990000;
}

.blue {
    color: #0000ff;
}

.orange {
    color: #ffc000;
}

.green {
    color: #92d050;
}

.red {
    color: #ff0000;
}

.skyblue {
    color: #bbe0e3;
}

.gray {
    color: #7f7f7f;
}

.yellow {
    color: #ffc000;
}

.black {
    color: #000000;
}

.small {
    font-size: 90%;
}
 
.center {
    text-align: center;
}
 
/**
 * Theme : y (yshita2011)
 */
 
.ui-body-y {
	background: #ffffff;
	color: #000000;
	text-shadow: none;
	font-weight: normal;
}

.ui-body-y .ui-link-inherit {
	color: #990000;
}
 
.ui-bar-y {
	background: #ffffff;
	color: #990000;
	font-weight: normal;
}
.ui-bar-y,
.ui-bar-y input,
.ui-bar-y select,
.ui-bar-y textarea,
.ui-bar-y button {
    font-family: 'Artbank Gothic', Helvetica, Arial, sans-serif;
}
.ui-bar-y .ui-link-inherit {
	color: #990000;
}
.ui-bar-y .ui-link {
	color: #990000;
	font-weight: normal;
}
.ui-bar-y h1.ui-title {
    text-align:             left;
    margin-left:            1em;
}


.ui-body-y,
.ui-body-y input,
.ui-body-y select,
.ui-body-y textarea,
.ui-body-y button {
    font-family: 'Artbank Gothic', Helvetica, Arial, sans-serif;
}
.ui-body-y .ui-link-inherit {
	color: #990000;
}
.ui-body-y .ui-link {
    color: #990000;
    font-weight: normal;
    white-space: normal;
}

.ui-btn-up-y {
	border: 1px solid #990000;
	background: #990000;
	font-weight: normal;
	color: #fff;
}
.ui-btn-hover-y {
	border: 1px solid #990000;
	background: #990000;
	font-weight: normal;
	color: #fff;
}
.ui-btn-down-y {
	border: 1px solid #990000;
	background: #990000;
	font-weight: normal;
	color: #fff;
}
.ui-btn-up-y,
.ui-btn-hover-y,
.ui-btn-down-y {
    font-family: 'Artbank Gothic', Helvetica, Arial, sans-serif;
	text-decoration: none;
}

